{
  "name": "new-pace-back",
  "version": "1.0.0",
  "main": "dist/app.js",
  "scripts": {
    "dev": "dotenv -e .env.development -- ts-node-dev --respawn --transpile-only src/server.ts",
    "prod": "dotenv -e .env.production -- ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "dotenv -e .env.test -- jest",
    "prisma:migrate": "prisma migrate dev --name init --preview-feature",
    "prisma:generate": "prisma generate",
    "lint": "eslint 'src/**/*.{js,ts}' --fix",
    "format": "prettier --write 'src/**/*.{js,ts,json,md}'",
    "initialize-db": "ts-node src/initialize.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Backend for New Pace project",
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "axios": "^1.7.3",
    "cors": "^2.8.5",
    "dotenv-cli": "^7.4.2",
    "express": "^4.19.2",
    "jsdom": "^24.1.1",
    "multer": "^1.4.5-lts.1",
    "new-pace-back": "file:",
    "node-html-parser": "^6.1.13",
    "sequelize": "^6.37.3",
    "sqlite3": "^5.1.7",
    "supertest": "^7.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "xlsx": "^0.18.5",
    "xpath": "^0.0.34"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.7",
    "@types/multer": "^1.4.11",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/xlsx": "^0.0.36",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma": "^5.17.0",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
